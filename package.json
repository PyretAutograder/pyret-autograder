{
  "name": "pyret-autograder-cli",
  "version": "0.0.1-pre13",
  "description": "A cli tool to test and run Pyret autograders, using the same configuration expected by an adaptor.",
  "author": "Owen Duckham <me@ironmoon.dev>",
  "license": "AGPL-3.0-or-later",
  "type": "module",
  "bin": {
    "pyret-autograder": "dist/index.js",
    "pyret-autograder-cli": "dist/index.js"
  },
  "scripts": {
    "build": "npm run build_typescript && npm run build_pawtograder",
    "build_typescript": "swc bin -d dist",
    "build_pawtograder": "npx --no pyret -- --builtin-js-dir node_modules/pyret-lang/src/js/trove/ -p src/pawtograder.arr -o src/pawtograder.cjs -ck",
    "prepack": "npm run build"
  },
  "dependencies": {
    "canvas": "^3.1.2",
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "cross-fetch": "^4.1.0",
    "fast-csv": "^5.0.5",
    "js-md5": "^0.8.3",
    "js-sha256": "^0.11.1",
    "pyret-autograder-pawtograder": "0.0.1-pre13",
    "q": "^2.0.3",
    "resolve": "^1.22.10",
    "s-expression": "^3.1.1",
    "seedrandom": "^3.0.5",
    "source-map": "^0.7.6",
    "vega": "^6.1.2",
    "yaml": "^2.8.0"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "pyret-npm": "^0.0.50",
    "swc": "^1.0.11"
  }
}
