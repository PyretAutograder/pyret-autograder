{
  "name": "pyret-autograder-cli",
  "version": "0.0.1-pre.22",
  "description": "A cli tool to test and run Pyret autograders, using the same configuration expected by an adaptor.",
  "author": "Owen Duckham <me@ironmoon.dev>",
  "license": "AGPL-3.0-or-later",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PyretAutograder/pyret-autograder-cli.git"
  },
  "bin": {
    "pyret-autograder": "dist/index.js",
    "pyret-autograder-cli": "dist/index.js"
  },
  "scripts": {
    "build": "npm run build_typescript && npm run build_pawtograder",
    "build_typescript": "swc bin -d dist",
    "build_pawtograder": "npm run build_pawtograder:prepare && npm run build_pawtograder:pyret",
    "build_pawtograder:prepare": "./scripts/prepare.sh",
    "build_pawtograder:pyret": "./scripts/compile-pawtograder.sh",
    "prepack": "npm run build"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "pyret-autograder": "0.0.1-pre.38",
    "pyret-autograder-pawtograder": "0.0.1-pre.32",
    "yaml": "^2.8.0",
    "canvas": "^3.1.0",
    "cross-fetch": "^4.1.0",
    "fast-csv": "^5.0.2",
    "js-md5": "^0.8.3",
    "js-sha256": "^0.5.0",
    "q": "~1.0.1",
    "resolve": "^1.22.10",
    "s-expression": "~2.2.0",
    "seedrandom": "~2.3.10",
    "source-map": "^0.5.7",
    "vega": "^6.1.2"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "swc": "^1.0.11"
  }
}
