{
  "name": "pyret-autograder",
  "version": "0.0.1-pre.42",
  "description": "Pyret autograder.",
  "author": "Owen Duckham <me@ironmoon.dev>",
  "license": "LGPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/PyretAutograder/pyret-autograder"
  },
  "main": "src/main.arr",
  "dependencies": {
    "pyret-lang": "npm:@ironm00n/pyret-lang@0.0.5",
    "pyret-npm": "npm:@ironm00n/pyret-npm@0.0.3"
  },
  "scripts": {
    "pyret-trove": "npx pyret --builtin-js-dir node_modules/pyret-lang/src/js/trove/",
    "test": "npm run pyret-trove -- -p tests/main.arr",
    "integration:gcd": "npm run pyret-trove -- -p tests/integration/inspect-gcd.arr",
    "integration:fold": "npm run pyret-trove -- -p tests/integration/inspect-fold.arr",
    "integration:tower": "npm run pyret-trove -- -p tests/integration/inspect-tower.arr",
    "clean": "rm -rf -- ./pyret"
  },
  "devDependencies": {
    "@types/node": "^24.2.0"
  }
}
